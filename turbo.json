{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "dev": {
      "cache": false,
      "persistent": true
    },
    "lint": { "outputs": [] },
    "typecheck": { "outputs": [] },
    "test": { "outputs": [] },
    "test:ci": { "outputs": [] },
    "build": { "dependsOn": ["^build"], "outputs": ["dist/**", ".next/**", "out/**"] },
    "tauri:dev": { "cache": false, "persistent": true },
    "tauri:build": { "dependsOn": ["build"], "outputs": ["src-tauri/target/**"] }
  }
}

